sayilar=[1,3,5,6,7,8]

sonuc=sum(sayilar)
sonuc=sum(sayilar,30)

urun=[{"title":"phone 1","price":5000},
      {"title":"phone 2","price":6000},
      {"title":"phone 3","price":7000}]
toplam=sum([x["price"] for x in urun])
adet=len([x for x in urun if x["price"]> 0])
sonuc= toplam / adet
print(sonuc)